{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "python",
          "cmd": "python",
          "args": true
        }
      ]
    },
    "shell:allow-open",
    "dialog:default",
    "fs:default"
  ]
}